<% form_for(@archivo, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <div>
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </div>
  <div>
    <%= f.label :descripcion %><br />
    <%= f.text_area :descripcion %>
  </div>
  <div>
    <%= f.label :archivo_excel %><br />
    <%= f.file_field :archivo_excel %>
  </div>
  <div>
    <%= f.submit 'Salvar', :class => 'ui-state-active' %>
  </div>
<% end %>

