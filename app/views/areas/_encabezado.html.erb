<% f.inputs do %>
  <li class="string">
    <label for="id">Celda Inicial</label>
    <span id="span_encabezado_celda_inicial"><%= @area.formato_excel(:encabezado_celda_inicial) %></span>
  </li>
  <li class="string">
    <label for="id">Celda Final</label>
    <span id="span_encabezado_celda_final"><%= @area.formato_excel(:encabezado_celda_final) %></span>
  </li>
  <%= f.input :encabezado_celda_inicial, :as => :hidden %>
  <%= f.input :encabezado_celda_final, :as => :hidden %>
<% end %>
